  </ItemGroup>

  <ItemGroup>
    <Compile Update="Views\voirMotDePasse.xaml.cs">
      <DependentUpon>voirMotDePasse.xaml</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
